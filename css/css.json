{
  "Reset rules": {
    "prefix": "reset",
    "body": [
      "body,",
      "body * {",
      "\tmargin: 0;",
      "\tpadding: 0;",
      "\tvertical-align: top;",
      "\tbox-sizing: border-box;",
      "}",
      "",
      "body {",
      "\tfont-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;",
      "}"
    ]
  },
  "Mini CSS reset rules": {
    "prefix": "teve",
    "body": [
      "body,",
      "body * {",
      "\tmargin: 0;",
      "\tpadding: 0;",
      "\tvertical-align: top;",
      "\tbox-sizing: border-box;",
      "}",
      "",
      "body {",
      "\tfont-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif;",
      "}"
    ]
  },
  "Sventa Marija": {
    "prefix": "marija",
    "body": ["display: inline-block;", "float: left;", "width: 100%;"]
  },
  "Centriukas": {
    "prefix": "centriukas",
    "body": [
      "position: absolute;",
      "top: 50%;",
      "left: 50%;",
      "transform: translate(-50%, -50%);"
    ]
  }
}
